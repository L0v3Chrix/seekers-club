// ========================================
// HELPER FUNCTIONS
// ========================================

/**
 * Get the 33 final selected gifts
 */
export function getFinalGifts(): SpiritualGift[] {
  return SPIRITUAL_GIFTS.filter(gift => gift.isFinal === true)
}

/**
 * Get backup gifts (67 alternates)
 */
export function getBackupGifts(): SpiritualGift[] {
  return SPIRITUAL_GIFTS.filter(gift => gift.isFinal !== true)
}

/**
 * Get gifts by category
 */
export function getGiftsByCategory(category: SpiritualGift['category']): SpiritualGift[] {
  return SPIRITUAL_GIFTS.filter(gift => gift.category === category)
}

/**
 * Get gifts by teacher
 */
export function getGiftsByTeacher(teacher: string): SpiritualGift[] {
  return SPIRITUAL_GIFTS.filter(gift => gift.teacher === teacher)
}

/**
 * Get a random gift that hasn't been seen
 */
export function getRandomUnseenGift(seenIds: string[]): SpiritualGift | null {
  const unseenGifts = getFinalGifts().filter(gift => !seenIds.includes(gift.id))

  if (unseenGifts.length === 0) return null

  const randomIndex = Math.floor(Math.random() * unseenGifts.length)
  return unseenGifts[randomIndex]
}

